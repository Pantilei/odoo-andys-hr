    <record id="hr_job_group_action" model="ir.actions.act_window">
        <field name="name">Available Job Name</field>
        <field name="res_model">restaurant_hr.hr_job_group</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>

</odoo>