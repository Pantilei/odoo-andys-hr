<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="hr_job_group_view_tree" model="ir.ui.view">
        <field name="name">hr_job_group.view.tree</field>
        <field name="model">restaurant_hr.hr_job_group</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name" />
